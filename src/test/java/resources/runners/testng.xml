<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1">
    <listeners>
        <listener class-name="com.tutorial.demo.customlisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"/>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"/>

    </listeners>
    <test name="Desktop Test">
        <classes>
            <class name="com.tutorial.demo.testsuite.DesktopTest"/>
            <class name="com.tutorial.demo.testsuite.HomePageTest"/>
            <class name="com.tutorial.demo.testsuite.LaptopAndNotebooksTest"/>
            <class name="com.tutorial.demo.testsuite.MyAccountTest"/>
        </classes>
    </test>
</suite>